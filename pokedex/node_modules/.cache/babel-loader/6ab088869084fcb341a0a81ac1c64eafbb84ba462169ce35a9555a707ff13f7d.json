{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { pokeapi } from \"@/api/pokeapi\";\nexport default {\n  data() {\n    return {\n      pokemonData: {}\n    };\n  },\n  methods: {\n    async searchPokemon() {\n      try {\n        const response = await fetch(`${pokeapi}?limit=50`);\n        const data = await response.json();\n        const pokemons = [];\n        for (const result of data.results) {\n          const pokemonResponse = await fetch(result.url);\n          const pokemonData = await pokemonResponse.json();\n\n          // Manipule os dados do Pokémon aqui, como alterar a capitalização do nome\n          const capitalizedPokemonName = pokemonData.name.charAt(0).toUpperCase() + pokemonData.name.slice(1);\n          pokemonData.name = capitalizedPokemonName;\n          pokemons.push(pokemonData);\n        }\n        this.pokemons = pokemons;\n        console.log(pokemons);\n      } catch (error) {\n        alert(\"Pokemon data could not be loaded\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["pokeapi","data","pokemonData","methods","searchPokemon","response","fetch","json","pokemons","result","results","pokemonResponse","url","capitalizedPokemonName","name","charAt","toUpperCase","slice","push","console","log","error","alert"],"sources":["/home/ivy/Documentos/pokedex/src/components/PokemonCards.vue"],"sourcesContent":["<template>\n    <div>\n        <ul class=\"type\">\n                <h2>Type:</h2>\n                <li\n                    v-for=\"(type, index) in pokemonData.types\"\n                    :key=\"index\"\n                    :class=\"type.type.name\"\n                >\n                    <span><h3>{{ type.type.name }}</h3></span>\n                </li>\n            </ul>\n    </div>\n  </template>\n  \n\n<script>\nimport { pokeapi } from \"@/api/pokeapi\";\nexport default {\n  data() {\n    return {\n      pokemonData: {},\n    };\n  },\n  methods: {\n    async searchPokemon() {\n      try {\n        const response = await fetch(`${pokeapi}?limit=50`);\n        const data = await response.json();\n\n        const pokemons = [];\n        for (const result of data.results) {\n          const pokemonResponse = await fetch(result.url);\n          const pokemonData = await pokemonResponse.json();\n\n          // Manipule os dados do Pokémon aqui, como alterar a capitalização do nome\n          const capitalizedPokemonName =\n            pokemonData.name.charAt(0).toUpperCase() +\n            pokemonData.name.slice(1);\n          pokemonData.name = capitalizedPokemonName;\n\n          pokemons.push(pokemonData);\n        }\n\n        this.pokemons = pokemons;\n        console.log(pokemons);\n      } catch (error) {\n        alert(\"Pokemon data could not be loaded\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Estilos para o componente PokemonList.vue */\n</style>\n"],"mappings":";AAiBA,SAASA,OAAM,QAAS,eAAe;AACvC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAE,GAAEN,OAAQ,WAAU,CAAC;QACnD,MAAMC,IAAG,GAAI,MAAMI,QAAQ,CAACE,IAAI,CAAC,CAAC;QAElC,MAAMC,QAAO,GAAI,EAAE;QACnB,KAAK,MAAMC,MAAK,IAAKR,IAAI,CAACS,OAAO,EAAE;UACjC,MAAMC,eAAc,GAAI,MAAML,KAAK,CAACG,MAAM,CAACG,GAAG,CAAC;UAC/C,MAAMV,WAAU,GAAI,MAAMS,eAAe,CAACJ,IAAI,CAAC,CAAC;;UAEhD;UACA,MAAMM,sBAAqB,GACzBX,WAAW,CAACY,IAAI,CAACC,MAAM,CAAC,CAAC,CAAC,CAACC,WAAW,CAAC,IACvCd,WAAW,CAACY,IAAI,CAACG,KAAK,CAAC,CAAC,CAAC;UAC3Bf,WAAW,CAACY,IAAG,GAAID,sBAAsB;UAEzCL,QAAQ,CAACU,IAAI,CAAChB,WAAW,CAAC;QAC5B;QAEA,IAAI,CAACM,QAAO,GAAIA,QAAQ;QACxBW,OAAO,CAACC,GAAG,CAACZ,QAAQ,CAAC;MACvB,EAAE,OAAOa,KAAK,EAAE;QACdC,KAAK,CAAC,kCAAkC,CAAC;MAC3C;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}