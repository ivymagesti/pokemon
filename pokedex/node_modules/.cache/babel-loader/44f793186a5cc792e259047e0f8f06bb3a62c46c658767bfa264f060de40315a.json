ts) {\n            const pokemonResponse = await fetch(result.url);\n            const pokemonData = await pokemonResponse.json();\n  \n            // Manipule os dados do Pokémon aqui, como alterar a capitalização do nome\n            const capitalizedPokemonName =\n              pokemonData.name.charAt(0).toUpperCase() +\n              pokemonData.name.slice(1);\n            pokemonData.name = capitalizedPokemonName;\n  \n            this.pokemons.push(pokemonData); // Adicione o Pokémon à matriz\n          }\n  \n          this.offset += this.limit; // Atualize o offset para a próxima página\n        } catch (error) {\n          alert(\"Pokemon data could not be loaded\");\n        }\n      },\n    },\n    mounted() {\n      // Carregue os primeiros Pokémon quando o componente for montado\n      this.loadMore();\n    },\n  };\n  </script>\n  \n  <style scoped>\n  /* Estilos para o componente PokemonList.vue */\n  </style>\n  "],"mappings":";;uBACIA,mBAAA,CAKM,e,kBAJJA,mBAAA,CAEMC,SAAA,QAAAC,WAAA,CAFqCC,KAAA,CAAAC,QAAQ,EAAnBC,OAAO;yBAAvCL,mBAAA,CAEM;MAFDM,KAAK,EAAC,aAAa;MAA8BC,GAAG,EAAEF,OAAO,CAACG;QACjEC,mBAAA,CAA2B,YAAAC,gBAAA,CAApBL,OAAO,CAACG,IAAI,iB;kCAErBC,mBAAA,CAA4C;IAAnCE,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,QAAA,IAAAD,QAAA,CAAAC,QAAA,IAAAF,IAAA,CAAQ;KAAE,WAAS,E"},"metadata":{},"sourceType":"module","externalDependencies":[]}