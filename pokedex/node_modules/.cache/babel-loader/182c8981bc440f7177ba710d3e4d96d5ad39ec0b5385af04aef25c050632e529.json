{"ast":null,"code":"import { pokeapi } from \"@/api/pokeapi\";\nimport axios from \"axios\";\nexport default {\n  name: 'PokemonCards',\n  data() {\n    return {\n      pokemonData: {},\n      pokemonID: \"\"\n    };\n  },\n  methods: {\n    getPokemons() {\n      axios.get(\"https://pokeapi.co/api/v2/pokemon?limit=493\").then(response => {\n        this.pokemons = response.data.results.map(pokemon => {\n          pokemon.name = pokemon.name.charAt(0).toUpperCase() + pokemon.name.slice(1);\n          return pokemon;\n        });\n        this.visiblePokemons = this.pokemons.slice(0, 20);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    async searchPokemon() {\n      try {\n        const pokemonToFind = await fetch(`${pokeapi}/${this.pokemonID}`);\n        const pokemon = await pokemonToFind.json();\n        this.pokemonData = pokemon;\n        this.pokemonData.name = pokemon.name.charAt(0).toUpperCase() + pokemon.name.slice(1);\n        console.log(pokemon);\n        return pokemon;\n      } catch (error) {\n        alert(\"Pokemon was not found\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["pokeapi","axios","name","data","pokemonData","pokemonID","methods","getPokemons","get","then","response","pokemons","results","map","pokemon","charAt","toUpperCase","slice","visiblePokemons","catch","error","console","log","searchPokemon","pokemonToFind","fetch","json","alert"],"sources":["/home/ivy/Documentos/pokedex/src/components/PokemonCards.vue"],"sourcesContent":["<template>\n    <container class=\"container\">\n      <div class=\"row mx-0 d-flex justify-center\">\n        <div\n          class=\"col-4\"\n          v-for=\"pokemon in visiblePokemons\"\n          :key=\"getId(pokemon)\"\n        >\n          <div class=\"pokedex\">\n            <div class=\"cartoes-pokemon\">\n              <div class=\"cartao-pokemon aberto\">\n                <div class=\"cartao-topo\">\n                  <div class=\"detalhes\">\n                    <h2 class=\"nome\">{{ pokemon.name }}</h2>\n                    <span> {{ getId(pokemon) }} </span>\n                  </div>\n\n                  <div class=\"cartao-imagem\">\n                    <img\n                      :src=\"`https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/${getId(\n                        pokemon\n                      )}.png`\"\n                      :alt=\"pokemon.name\"\n                      width=\"70%\"\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"d-flex justify-center\">\n        <button type=\"button\" class=\"btn btn-outline-secondary\" @click=\"showMore()\">Mostrar Mais</button>\n      </div>    \n    </container>\n</template>\n\n<script>\nimport { pokeapi } from \"@/api/pokeapi\";\nimport axios from \"axios\";\nexport default{\n    name: 'PokemonCards',\n\n    data() {\n    return {\n      pokemonData: {},\n      pokemonID: \"\",\n    };\n  },\n\n  methods: {\n    getPokemons() {\n    axios\n    .get(\"https://pokeapi.co/api/v2/pokemon?limit=493\")\n    .then((response) => {\n      this.pokemons = response.data.results.map((pokemon) => {\n        pokemon.name =\n          pokemon.name.charAt(0).toUpperCase() + pokemon.name.slice(1);\n        return pokemon;\n      });\n      this.visiblePokemons = this.pokemons.slice(0, 20);\n    })\n    .catch((error) => {\n      console.log(error);\n    });\n    },\n\n    async searchPokemon() {\n      try {\n        const pokemonToFind = await fetch(`${pokeapi}/${this.pokemonID}`);\n        const pokemon = await pokemonToFind.json();\n        this.pokemonData = pokemon;\n        this.pokemonData.name = pokemon.name.charAt(0).toUpperCase() + pokemon.name.slice(1);\n        console.log(pokemon);\n        return pokemon;\n      } catch (error) {\n        alert(\"Pokemon was not found\");\n      }\n    },\n  },\n\n \n\n}\n</script>