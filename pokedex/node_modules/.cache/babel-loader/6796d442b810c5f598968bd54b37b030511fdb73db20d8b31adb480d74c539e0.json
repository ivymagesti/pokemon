Find = await fetch(`${pokeapi}/${this.pokemonID}`)\n          const pokemon = await pokemonToFind.json()\n          this.pokemonData = pokemon\n          console.log(pokemon)\n          return pokemon\n        } catch (error) {\n          alert('Pokemon was not found')\n        }\n      }\n    },\n  }\n  </script>\n  \n  <style scoped>\n  </style>\n  "],"mappings":"AAiCE,SAASA,OAAM,QAAS,eAAe;AAEvC,eAAe;EACbC,IAAI,EAAE,eAAe;EAErBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC,CAAC;MACfC,SAAS,EAAE;IACb;EACF,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,aAAY,GAAI,MAAMC,KAAK,CAAE,GAAER,OAAQ,IAAG,IAAI,CAACI,SAAU,EAAC;QAChE,MAAMK,OAAM,GAAI,MAAMF,aAAa,CAACG,IAAI,CAAC;QACzC,IAAI,CAACP,WAAU,GAAIM,OAAM;QACzBE,OAAO,CAACC,GAAG,CAACH,OAAO;QACnB,OAAOA,OAAM;MACf,EAAE,OAAOI,KAAK,EAAE;QACdC,KAAK,CAAC,uBAAuB;MAC/B;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}